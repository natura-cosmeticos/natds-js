  },
  "scripts": {
    "build": "yarn build:clean && tsc -p ./tsconfig.json && yarn rollup:build",
    "build:clean": "rm -rf ./dist",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "prepublish": "npm run snyk-protect",
    "rollup:build": "rollup -c",
    "snyk-protect": "snyk protect",
    "start": "yarn build -w",
    "test": "yarn test:unit:coverage",
    "test:ci": "yarn test --json --outputFile=.jest-test-results.json && codecov",
    "test:unit": "jest",
    "test:unit:coverage": "yarn test:unit --coverage --silent",
    "test:unit:coverage:open": "open coverage/lcov-report/index.html",
    "test:unit:generate": "yarn test:unit --json --outputFile=.jest-test-results.json || true",
    "test:unit:watch": "yarn test:unit --watch",
    "verify": "yarn lint && yarn test -u",
    "verify:ci": "yarn lint && yarn test",
    "watch": "yarn start"
  },
