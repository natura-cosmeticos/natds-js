  "scripts": {
    "build": "yarn clean && tsc -p ./tsconfig.json && yarn rollup:build && yarn storybook:build",
    "clean": "rm -rf ./dist",
    "open:coverage": "open coverage/lcov-report/index.html",
    "prepare": "yarn snyk-protect",
    "pretest": "yarn build",
    "rollup:build": "rollup -c",
    "snyk-protect": "snyk protect",
    "start": "yarn build -w",
    "storybook:build": "build-storybook --quiet",
    "storybook:build:local": "yarn storybook:build -o dist-docs",
    "storybook:build:ci": "yarn storybook:build",
    "storybook:start": "start-storybook -p 5000",
    "storybook:test": "build-storybook -o dist-test --quiet && rm -rf dist-test",
    "storybook:watch": "yarn storybook:start",
    "test": "yarn testc",
    "testc": "jest --coverage",
    "test:ci": "yarn test && codecov",
    "test:output": "yarn test --json --outputFile=.jest-test-results.json || true",
    "testw": "yarn test --watch",
    "watch": "yarn start"
  },
