  "main": "dist/index.js",
  "browser": "dist/umd/index.js",
  "jsdelivr": "dist/umd/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "unpkg": "dist/umd/index.js",
  "repository": {
    "directory": "packages/icons",
    "type": "git",
    "url": "https://github.com/natura-cosmeticos/natds.git"
  },
  "directories": {
    "doc": "./docs",
    "lib": "./dist"
  },
  "scripts": {
    "build": "yarn clean && yarn minify-svg-files && yarn create-font-files && yarn build-typescript && yarn copy-fonts && yarn copy-css && yarn generate-typings && yarn generate-json-type && yarn copy-typings && yarn generate-umd",
    "build-typescript": "tsc -p ./tsconfig.json",
    "clean": "rm -rf dist && rm -rf src/assets/cleaned/* && rm -rf src/fonts/*",
    "copy-css": "cp -r ./src/*.css* ./dist",
    "copy-fonts": "cp -r ./src/fonts ./dist",
    "copy-typings": "cp -r ./src/*.d.ts ./dist",
    "create-font-files": "node ./scripts/svgToFont.js",
    "generate-json-type": "node ./scripts/generateJsonType.js",
    "generate-typings": "tcm -o src src",
    "generate-umd": "rollup -c",
    "minify-svg-files": "node ./scripts/svgCleaner.js",
    "prepare": "yarn snyk-protect",
    "pretest": "yarn build",
    "snyk-protect": "snyk protect",
    "start": "yarn build --watch",
    "test": "jest --coverage",
    "testc": "yarn test",
    "test:ci": "yarn test",
    "watch": "yarn start"
  },
  "config": {},
  "devDependencies": {
    "@babel/core": "7.11.4",
    "@babel/preset-env": "7.11.5",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-typescript": "6.0.0",
    "@rollup/plugin-url": "5.0.1",
    "babel-jest": "26.3.0",
    "codecov": "3.7.2",
    "glob": "7.1.6",
    "jest": "26.4.2",
    "json-to-ts": "1.7.0",
    "rollup": "2.26.7",
    "snyk": "^1.341.1",
    "svgo": "1.3.2",
    "tslib": "2.0.1",
    "typed-css-modules": "0.6.4",
    "typescript": "4.0.2",
    "webfont": "9.0.0",
    "yarn": "1.22.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
